<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateUserController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="firstNameLabel" layoutX="173.0" layoutY="46.0" text="Voornaam:" />
            <Label fx:id="lastNameLabel" layoutX="162.0" layoutY="78.0" text="Achternaam:" />
            <Label layoutX="205.0" layoutY="165.0" text="Rol:" />
            <Label fx:id="emailLabel" layoutX="196.0" layoutY="118.0" text="Email:" />
            <TextField fx:id="firstNameText" layoutX="246.0" layoutY="42.0" />
            <TextField fx:id="lastNameText" layoutX="246.0" layoutY="75.0" />
            <TextField fx:id="emailText" layoutX="246.0" layoutY="115.0" />
            <ComboBox fx:id="roleCombo" layoutX="246.0" layoutY="161.0" prefWidth="150.0" />
            <Button fx:id="createButton" layoutX="505.0" layoutY="311.0" mnemonicParsing="false" onAction="#createUser" text="Aanmaken" />
            <Label fx:id="errorLabel" layoutX="246.0" layoutY="200.0" text="------------------------------" />
         </children>
    </AnchorPane>
  </children>
</VBox>
